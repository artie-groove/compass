extends ../templates/page-light.tpl.pug

block request-button
	a.nav-link.request(data-toggle='modal' data-target='#requestCruise') Оставить заявку
		span.d-md-none.d-lg-inline  на тур

append scripts-delayed
	script(src='assets/js/pickmeup.min.js')
	script(src='//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.js')


block content
	#content-cruise.layout-sidebar
		.container
			.row
				.col-md-4.col-lg-3.order-1.order-md-0
					h3 Мы познакомим вас с&nbsp;Японией
					+staff-card('totchilina', 'Точилина Кристина', 'г. Благовещенск', true)
					+staff-card('matviytchuck', 'Матвийчук Юлия', 'г. Благовещенск', true)
					.btn.rounded-0.btn-request(href='#' data-toggle="modal" data-target="#requestConsult") Заявка на подбор подходящих круизов
				.col-md-8.col-lg-9.order-0.order-md-1
					nav
						#nav-cruise.nav.nav-tabs.flex-nowrap(role='tablist')
							a#nav-catalogue-tab.nav-item.nav-link.active(data-toggle='tab', href='#nav-catalogue', role='tab', aria-controls='nav-catalogue', aria-selected='true')
								div Каталог
							a#nav-early-booking-tab.nav-item.nav-link(data-toggle='tab', href='#nav-early-booking', role='tab', aria-controls='nav-early-booking', aria-selected='false')
								div Раннее бронирование
							a#nav-promo-tab.nav-item.nav-link(data-toggle='tab', href='#nav-promo', role='tab', aria-controls='nav-promo', aria-selected='false')
								div Акции
							a#nav-howto-tab.nav-item.nav-link(data-toggle='tab', href='#nav-howto', role='tab', aria-controls='nav-howto', aria-selected='false')
								div Как выбрать круиз
					#nav-cruiseContent.tab-content
						#nav-catalogue.tab-pane.fade.show.active(role='tabpanel', aria-labelledby='nav-catalogue-tab')
							.cruise-gallery
								.container-fluid
									.row
										.col-md-4
											.cruise-gallery-header
												img.d-block(src='assets/img/cruises/cover.jpg')
										.col-md-8
											.cruise-gallery-description
												h5 Япония&nbsp;&mdash; восточный рай
												p Страна восходящего солнца Япония&nbsp;&mdash; кладезь утонченной экзотики. Суперсовременный Токио и&nbsp;живописная провинция, снежная шапка Фудзи и&nbsp;сакура в&nbsp;цвету, великолепные пляжи островов Окинава и&nbsp;древние памятники синтоизма&nbsp;&mdash; все о&nbsp;Японии: фото, карта, туры, цены и&nbsp;отзывы.
									.row
										.col-12
											hr.mb-4.mt-0
									.row
										each card, i in data.cruises.gallery
											.col-md-6.col-xl-4
												+cover-card(normalizeNumber(i+1), 'cruises/covers', card.title, card.subtitle, null, true)
									.row
										.col-12
											#top5
												h5 Топ-5 популярных круизов
												+cruise-card('01', 'Морской круиз Испания и Корсика')
												+cruise-card('02', 'Морской круиз по бухте Халонг Бей')
												+cruise-card('03', 'Речной круиз по Волге')

						#nav-early-booking.tab-pane.fade(role='tabpanel', aria-labelledby='nav-early-booking-tab')
							.card.text-white#earlyBookingHeader
								.card-img
									img(src='assets/img/cruises/bg-early-booking.jpg')
								.card-img-overlay
									.container-fluid
										.row
											.col-12.col-lg-9.col-xl-7
												h5.card-title 5 причин забронировать круиз заранее
												.card-text
													ul.list-positive.list-positive-simplified
														li Экономьте на&nbsp;стоимости. Цены предпродаж ниже сезонных
														li Вы гарантированно бронируете место на&nbsp;судне
														li Бюджет, который вы&nbsp;выделяете, зарезервирован
														li Запланированное путешествие дарит вам недели желанного предвкушения
														li У вас будет достаточно времени подумать, что взять с&nbsp;собой
							h2 2018
							+early-booking-offer('Восточный экспресс', { name: 'Рим-Монако-Барселона-Порту', id: '01' }, ['guide', 'promo'], { name: 'Royal Carribean', imgid: '01'})
							+early-booking-offer('Трансатлантик', { name: 'Барселона-Тенерифе-Рио-де-Жанейро', id: '02' }, ['new'], { name: 'Empress Ki', imgid: '02'})
							h2 2019
							+early-booking-offer('Восточный экспресс', { name: 'Рим-Монако-Барселона-Порту', id: '01' }, ['new'], { name: 'Royal Carribean', imgid: '01'})
							h2 2020
							+early-booking-offer('Трансатлантик', { name: 'Барселона-Тенерифе-Рио-де-Жанейро', id: '02' }, ['guide', 'promo'], { name: 'Empress Ki', imgid: '02'})
							+early-booking-offer('Восточный экспресс', { name: 'Рим-Монако-Барселона-Порту', id: '01' }, ['new'], { name: 'Royal Carribean', imgid: '01'})

						#nav-promo.tab-pane.fade(role='tabpanel', aria-labelledby='nav-promo-tab')
							+promo-card('01', 'Ребенок до 16-ти лет&nbsp;&mdash; БЕСПЛАТНО', '30.04.2019')
